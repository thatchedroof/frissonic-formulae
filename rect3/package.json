    "lint-staged": "16.1.5",
    "npm-run-all": "^4.1.5",
    "postcss": "8.5.6",
    "postcss-less": "6.0.0",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "stylelint": "16.23.1",
    "stylelint-config-standard": "39.0.0",
    "stylelint-less": "3.0.1",
    "stylelint-prettier": "5.0.3",
    "tailwindcss": "4.1.12",
