{
  "name": "rect3",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "run-s wasm-pack:release typecheck vite:build",
    "dev": "run-s wasm-pack:dev vite:dev",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "vite:build": "vite build",
    "vite:dev": "vite -c vite.config.dev.mts",
    "vite:preview": "vite preview",
    "lint": "eslint --quiet src --ext .ts,.tsx && stylelint src/**/*.less",
    "lint:fix": "eslint --quiet --fix src --ext .ts,.tsx && stylelint --fix src/**/*.less",
    "wasm-pack:dev": "wasm-pack build ./rect3 --dev --target web",
    "wasm-pack:release": "wasm-pack build ./rect3 --release --target web"
  },
  "dependencies": {
